<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.simple">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.simple.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".net.NormalTestActivity"/>
        <activity android:name=".net.HttpsTestActivity"/>
        <activity android:name=".image.ImageTestActivity"/>
        <activity android:name=".util.UtilTestActivity"/>
        <activity android:name=".util.SPUtilTestActivity"/>
        <activity android:name=".util.TaskManagerTestActivity" />
        <activity android:name=".util.PhoneInfoTestActivity" />
        <activity android:name=".util.DecimalUtilTestActivity" />
        <activity android:name=".util.TimeUtilTestActivity" />
        <activity android:name=".util.FileUtilTestActivity" />
        <activity android:name=".util.NotifyUtilTestActivity" />

    </application>

</manifest>